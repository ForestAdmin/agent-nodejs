{
  "author": "Nicolas Moreau",
  "bin": {
    "cloud-customizer": "./bin/run.js"
  },
  "dependencies": {
    "@forestadmin/agent": "^1.36.13",
    "@forestadmin/datasource-customizer": "^1.40.0",
    "@forestadmin/datasource-sql": "^1.7.43",
    "@oclif/command": "^1.8.36",
    "@oclif/core": "2.8.5",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-help": "2.1.3",
    "@oclif/plugin-plugins": "^2"
  },
  "description": "Utility to bootstrap and publish forest admin cloud projects customization",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@swc/core": "^1.3.104",
    "@types/node": "^18",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "oclif": "^4.1.3",
    "prettier": "^3.2.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "6.0.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ForestAdmin/cloud-customizer",
  "license": "GPL-3.0",
  "main": "",
  "name": "cloud-customizer",
  "oclif": {
    "bin": "cloud-customizer",
    "dirname": "cloud-customizer",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "ForestAdmin/cloud-customizer",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "version": "oclif readme && git add README.md",
    "makelinks": "yarn link @forestadmin/agent && yarn link @forestadmin/datasource-customizer && yarn link @forestadmin/datasource-sql && yarn link @forestadmin/datasource-sequelize && yarn link @forestadmin/datasource-toolkit && yarn link @forestadmin/forestadmin-client",
    "testrun": "yarn build && ./bin/run.js update-typings"
  },
  "version": "0.0.1",
  "bugs": "https://community.forestadmin.com/",
  "keywords": [
    "forestadmin",
    "cloud",
    "customization"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
