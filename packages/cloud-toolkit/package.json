{
  "name": "@forestadmin/cloud-toolkit",
  "version": "1.0.0-beta-cloud-toolkit.26",
  "description": "Utility to bootstrap and publish forest admin cloud projects customization",
  "dependencies": {
    "@forestadmin/agent": "1.37.0-beta-cloud-toolkit.1",
    "@forestadmin/datasource-customizer": "1.41.0",
    "@forestadmin/datasource-sql": "1.7.45",
    "adm-zip": "^0.5.10",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-ws": "^1.0.20",
    "axios": "^1.6.6",
    "commander": "^11.1.0",
    "dotenv": "^16.4.1",
    "forest-cli": "^5.0.0",
    "form-data": "^4.0.0",
    "graphql": "14.5.7",
    "graphql-tag": "^2.12.6",
    "ora": "^3.2.0",
    "sqlite3": "^5.1.7",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.txt"
  ],
  "homepage": "https://github.com/ForestAdmin/agent-nodejs/cloud-toolkit",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForestAdmin/agent-nodejs.git",
    "directory": "packages/cloud-toolkit"
  },
  "bin": {
    "forest-cloud": "dist/command.js"
  },
  "scripts": {
    "build": "tsc && yarn build:copy",
    "build:watch": "tsc --watch & nodemon --watch 'src/templates/*' --ext txt --exec yarn build:copy",
    "build:copy": "cp -r src/templates ./dist && cp package.json ./dist",
    "lint": "eslint src test",
    "test": "jest"
  },
  "keywords": [
    "forestadmin",
    "cloud",
    "customization"
  ],
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "nodemon": "^3.0.3",
    "log-symbols": "^4.1.0"
  }
}
