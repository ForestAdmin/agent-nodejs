  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@qiwi/multi-semantic-release": "^7.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fishery": "^2.2.2",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.3",
    "lerna": "^8.2.3",
    "markdown-link-check": "^3.10.3",
    "nock": "^13.3.0",
    "prettier": "^2.8.2",
    "prettier-eslint": "^15.0.1",
    "semantic-release-slack-bot": "^4.0.2",
    "ts-jest": "^29.0.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "bootstrap": "yarn install",
    "lerna:repair": "lerna repair",
    "build:watch": "lerna run --parallel --no-bail --no-prefix build:watch",
    "example:watch": "yarn nx run \"example\":start:watch",
    "build": "lerna run build",
    "clean": "lerna run clean && rm -rf coverage api-reference/",
    "docs": "typedoc",
    "lint": "lerna exec --parallel eslint src test",
    "lint:fix": "lerna exec --parallel eslint src test -- --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "workspaces": [
    "packages/*",
    "!packages/_example/*"
  ],
  "resolutions": {
    "express": "^4.21.1",
    "tar": "^6.2.1",
    "form-data": "^4.0.4",
    "socks": "^2.8.7",
    "micromatch": "^4.0.8",
    "@babel/helpers": "^7.26.10",
    "semantic-release": "^25.0.0"
  }
}
