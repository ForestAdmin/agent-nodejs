{
  "editor.rulers": [100, 120],
  "files.exclude": {
    ".github": true,
    ".husky": true,
    ".vscode": true,
    ".eslintrc.json": true,
    ".prettierrc": true,
    "yarn.lock": true,
    "lerna.json": true,
    "typedoc.json": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/jest.config.js": true,
    "**/LICENSE": true,
    "**/tsconfig.json": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true
  },
  "debug.javascript.terminalOptions": {
    "skipFiles": ["<node_internals>/**", "**/node_modules"]
  }
}
